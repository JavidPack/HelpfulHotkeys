			HelpfulHotkeysClientConfig: {
				DisplayName: Config

				EnableQuickUseItems11to19: {
					Label: Quick Use Item 11-19 Hotkeys
					Tooltip: Enable to allow these hotkeys. Keep disabled to keep keybinding menu clean.
				}

				SwapArmorInventorySlots: {
					Label: Swap Armor with Inventory Slots
					Tooltip:
						'''
						Customize the Swap Armor with Inventory slots here
						Count from 0. Use value from 0 to 49.
						Default values correspond to lower right slots in the inventory.
						'''
				}

				DashHotkeyDisablesDoubleTap: {
					Label: Disable Double Tap Dash Behavior
					Tooltip:
						'''
						Enable this to prevent the game from recognizing double taps for dashing.
						Use this if you intend to only use the dash hotkey.
						'''
				}

				DashHotkeyDisabledWhileInChest: {
					Label: Disable Dash Hotkey While Using Chest
					Tooltip:
						'''
						Enable this to prevent the dash hotkey from working while in a chest.
						Use this if your dash hotkey would conflict with chest operations, like shift, alt, or ctrl.
						'''
				}

				QuickUseConfigItem: {
					Label: Quick Use Config Item
					Tooltip:
						'''
						Customize the Quick Use Config Item here
						The specified item can be quickly used from your inventory by pressing the hotkey
						'''
				}

				ShowDeveloperInfoQueryModOrigin: {
					Label: Show Developer Info
					Tooltip:
						'''
						Enhances the Query Mod Origin hotkey to show internal names of Modded items.
						Vanilla ID values will also be shown.
						'''
				}
